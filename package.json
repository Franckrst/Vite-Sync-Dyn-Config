{
  "name": "vite-sync-dyn-config",
  "version": "0.0.1",
  "description": "Sync Dyn Config is a plugin for Vite that allows you to dynamically inject configurations into the generated HTML file. It supports conditional inclusion via SSI (Server Side Includes).",
  "author": "Franckrst",
  "license": "MIT",
  "homepage": "https://github.com/Franckrst/vite-sync-dyn-config",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Franckrst/vite-sync-dyn-config.git"
  },
  "bugs": "https://github.com/Franckrst/vite-sync-dyn-config/issues",
  "keywords": [
    "vite",
    "plugin",
    "SSI",
    "config"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "test": "jest --watchAll",
    "lint": "eslint ./src"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.14",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.5.3",
    "eslint": "^9.19.0",
    "typescript-eslint": "^8.23.0",
    "@eslint/js": "^9.19.0",
    "vite": "^6.0.11"
  }
}
